<%= render partial: "layouts/dbsidebar" %>
<div class="body-match"></div>
<div class="dashbox-match">
    <div class="container">
        <div class="row">
            <div class="col-sm-3" style="padding-left: 5px;">
                <div class="well" style="height: 350px;">
                    <div class="profile-img" style="height: 130px;">
                        <%= image_tag "profile1.jpg", width: "100%", height: "auto"%>
                        <i class="fa fa-refresh fa-spin fa-3x fa-fw loaders"></i>
                        <span class="sr-only"></span>
                    </div>
                    <div style="text-align: center; margin-top: 10px;margin-bottom: 5px; font-weight: 700; font-size: 15px; text-transform: capitalize">Yondu</div>
                    <div style="text-align: center; margin-bottom: 10px; color: #fff; background: #258f9f; width: 100px; border-radius: 10px; margin-left: 85px;">Badminton</div>
                    <div class="item-match" style="margin-bottom: 15px; margin-left: 30px;">
                      <div class="item-stats">
                        <p><i class="material-icons md-18" style="vertical-align:middle; font-size: 15px; line-height: 13px; padding-right:2px; color: #A9CF54;">fiber_manual_record</i>Active</p>
                      </div>
                      <div class="item-stats">
                        <p><i class="material-icons md-18" style="vertical-align:middle; font-size: 15px; line-height: 13px; padding-right:2px; color: #ccc;">fiber_manual_record</i>InActive</p>
                      </div>
                    </div>
                    <table class="dahsboard-matches">
