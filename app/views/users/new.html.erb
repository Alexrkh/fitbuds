<div class="container" >
    <div class="col-md-6" >
        <div id="logbox"  >
          <h1>Create an Account</h1>
            <%= form_for @user, id: "signup", url: users_path do |f| %>
              <%= f.email_field :email, placeholder: "Email", type: "email", class: "input pass" %>
              <%= f.password_field :password, placeholder: "Password", type: "password", class: "input pass" %>
              <%= f.submit "Sign Up", class: "inputButton" %>
            <% end %>
        </div>
    </div>
</div>
